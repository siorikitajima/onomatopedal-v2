<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <% if (nav === 'feat-single' || nav === 'v2' || nav === 'editor') { %>
        <link rel="stylesheet" href="../css/style.css">
        <link rel="stylesheet" href="../css/front.css">
        <link rel="stylesheet" href="../css/frontplayer.css">
        <link rel="icon"  type="image/png"  href="../images/OP_AppIcon.png" />
        <link rel="apple-touch-icon" sizes="128x128" href="../images/OP_AppIcon_128.png" />
        <link rel="icon" sizes="192x192" href="../images/OP_AppIcon_192.png" />
        <% if (nav === 'v2' && mobile) { %>
        <link rel="stylesheet" href="../css/mobileplayer.css">
        <%}%>
    <% } else {%>
        <link rel="stylesheet" href="css/style.css">
        <link rel="stylesheet" href="css/front.css">
        <link rel="stylesheet" href="css/frontplayer.css">
        <link rel="icon"  type="image/png"  href="images/OP_AppIcon.png" />
        <link rel="apple-touch-icon" sizes="128x128" href="images/OP_AppIcon_128.png" />
        <link rel="icon" sizes="192x192" href="images/OP_AppIcon_192.png" />
        <% if (nav === 'v2' && mobile) { %>
        <link rel="stylesheet" href="css/mobileplayer.css">
        <%}%>
    <%}%>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tone/14.7.77/Tone.js"></script>
    <meta property="og:type" content="music.song" />

    <% if (nav === 'v2') { %>
        <meta property="og:title" content="OnomatoPedal - <%= name %>" />
        <meta property="og:url" content="https://onomatopedal.com/v2/<%=name%>/" />
        <meta property="og:image" content="https://opv2-versioning.s3.us-west-2.amazonaws.com/<%= name %>/cover.jpg" />
        <title>OnomatoPedal - <%= name %> </title>
        <meta property="og:description" name="og:description" content="OnomatoPedal - <%= name %> is an Earthquaker Devices inspired online interactive music experience created by <%= pedal.artist %>.">
        <meta property="description" name="description" content="OnomatoPedal|<%= name %> is an Earthquaker Devices inspired online interactive music experience created by <%= pedal.artist %>.">
    <%  } else if (nav === 'feat-single') { %>
        <meta property="og:title" content="OnomatoPedal - <%= post.title %>" />
        <meta property="og:url" content="https://onomatopedal.com/feat/<%=post.name%>/" />
        <meta property="og:image" content="<%= post.featimg %>" />
        <title>OnomatoPedal - <%= post.title %> </title>
        <meta property="og:description" name="og:description" content="<%= post.desc %>">
        <meta property="description" name="description" content="<%= post.desc %>">
    <% } else {%>
        <title>OnomatoPedal | <%= title %> </title>
        <meta property="og:title" content="OnomatoPedal | <%= title %>" />
        <meta property="og:url" content="https://onomatopedal.com/<%=nav%>/" />
        <meta property="og:image" content="https://dev.onomatopedal.com/images/v1/OnomatoPedal_Cover_V2.jpg" />
        <meta property="og:description" name="og:description" content="OnomatoPedal is an Earthquaker Devices inspired online interactive experience that users can play music with by triggering sounds and visuals.">
        <meta property="description" name="description" content="OnomatoPedal is an Earthquaker Devices inspired online interactive experience that users can play music with by triggering sounds and visuals.">
    <%}%>
</head>