<html lang="en">
    <%- include('./partials/frontHead.ejs')  %> 
<body>
    <%- include('./partials/frontNav.ejs')  %> 

    <div class="featBanner">
        <p>Check Out V2 Demo!</p>
        <h1>Astral Destiny + Flutter + PatternBased</h1>
        <a href="/v2demo"><button class="lineBtn">Play</button></a>
    </div>
    <div class="grid-container">
        <a href="/v2demo">
            <div class="pedal-cover">
                <div class="diamond"></div>
                <p class="onoText">Flutter</p>
                <img  class="pedalImg" src="../images/pedals/astraldestiny.png" alt="pedal">               
                 <div class="pedal-name">
                    <h2>Flutter</h2>
                    <h2>Astral Destiny</h2>
                    <h2>PatternBased</h2>
                </div>
            </div>
        </a>
    <% for (var i = 0; i < pedalListing.length; i++) {%>
        <a href="/v1/<%= pedalListing[i].onomoid %>">
        <div class="pedal-grid">
            <img src= <%= pedalListing[i].image %> />
            <div class="pedal-name">
                <h2><%= pedalListing[i].onomoname %></h2>
                <h2><%= pedalListing[i].pedalFull %></h2>
                <h2><%= pedalListing[i].artist %></h2>
            </div>
        </div>
        </a>
    <% } %>
    </div>
    <%- include('./partials/pedal-cover.ejs')  %> 
</body>
</html>